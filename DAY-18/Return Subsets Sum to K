vector<vector<int>> ans;
void solve(vector<int> arr, int i, int k,vector<int> &ds){
    if(i==arr.size()){
        if(k==0) ans.push_back(ds);
        return;
    }
    ds.push_back(arr[i]);
    solve(arr,i+1,k-arr[i],ds);
    ds.pop_back();
    solve(arr,i+1,k,ds);
}
vector<vector<int>> findSubsetsThatSumToK(vector<int> arr, int n, int k)
{
    // Write your code here.
    vector<int> ds;
    solve(arr,0,k,ds);
    return ans;
}
