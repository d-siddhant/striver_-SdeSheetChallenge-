#include <bits/stdc++.h> 
int maximumProfit(vector<int> &prices){
    // Write your code here.
    int max=INT_MIN;
    int min=prices[0];
    for(int i=0;i<prices.size();i++){
        int money = prices[i] - min;
          if(money > max) max = money;
           if(prices[i] < min) min = prices[i];
    }
    return max;
}
