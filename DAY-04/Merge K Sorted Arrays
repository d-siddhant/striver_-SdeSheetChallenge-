#include<bits/stdc++.h>
vector<int> mergeKSortedArrays(vector<vector<int>>&A, int k)
{
    // Write your code here. 
    priority_queue<int,vector<int>,greater<int>> pq;
    for(int i=0;i<k;i++){
        for(int j=0;j<A[i].size();j++) pq.push(A[i][j]);
        }
    vector<int> ans;
    while(!pq.empty()){
        ans.push_back(pq.top());
        pq.pop();
    }
    return ans;
}
