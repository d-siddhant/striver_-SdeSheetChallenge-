#include <bits/stdc++.h> 
vector<int> majorityElementII(vector<int> &arr)
{
    // Write your code here.
    vector<int> ans;
    unordered_map<int,int> mp;
    for(int i=0;i<arr.size();i++) mp[arr[i]]++;
    int k=floor(arr.size()/3);
    for(auto it:mp)
    {
        if(it.second>k) ans.push_back(it.first);
    }
    return ans;

}
