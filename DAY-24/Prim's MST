#include<bits/stdc++.h> 
vector<pair<pair<int, int>, int>> calculatePrimsMST(int n, int m, vector<pair<pair<int, int>, int>> &g)
{
    // Write your code here.
    vector<pair<int,int>> adj[n+1];
    for(int i=0;i<m;i++){
        int u=g[i].first.first;
        int v=g[i].first.second;
        int w=g[i].second;
        adj[u].push_back({v,w});
        adj[v].push_back({u,w});
       }
    vector<int> parent(n+1,-1),key(n+1,INT_MAX);
    vector<bool> mst(n+1,false);
    key[1]=0;
    parent[1]=-1;
    for(int c=0;c<n-1;c++){
        int mini=INT_MAX,u;
        for(int i=1;i<=n;i++){
            if(mst[i]==false and key[i]<mini) {
                mini=key[i];
                u=i;
            }
        }
            mst[u]=true;
            for(auto it:adj[u]){
                int v=it.first;
                int w=it.second;
                if(!mst[v] and w<key[v]){
                    parent[v]=u;
                    key[v]=w;
                }
            }
        }
    
    vector<pair<pair<int, int>, int>> ans;
    for(int i=2;i<=n;i++){
        ans.push_back({{parent[i],i},key[i]});
    }
    return ans;
    
}
