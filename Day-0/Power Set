void solve(int i,vector<vector<int>> &ans, vector<int> &ds,vector<int> &v){
    if(i==v.size()){
        ans.push_back(ds);
        return;
    }
    ds.push_back(v[i]);
    solve(i+1,ans,ds,v);
    ds.pop_back();
    solve(i+1,ans,ds,v);
}

vector<vector<int>> pwset(vector<int>v)
{
    //Write your code here
    vector<vector<int>> ans;
    vector<int> ds;
    solve(0,ans,ds,v);
    return ans;
}
