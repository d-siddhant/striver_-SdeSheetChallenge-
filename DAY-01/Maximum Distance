int maxDiff(vector<int> &a, int n)
{
    // Write your code here.
    int maxi=  -2147483647 - 1;
    int ans=0;
    for(int i=0;i<n;i++){
        ans=0;
        for(int j=n-1;j>=i;j--){
            if(a[j]>=a[i]) {
                ans=j-i;
                break;
            }
        }
        maxi=max(maxi,ans);
    }
    return maxi;
}

