void solve(vector<int> &ds,vector<int> &num,int i,int sum){
    if(i==0) {
        ds.push_back(sum);
        return;
    }
  
     solve(ds,num,i-1,sum);
      sum+=num[i-1];
     solve(ds,num,i-1,sum);
      sum-=num[i-1];
    
}
vector<int> subsetSum(vector<int> &num)
{
    // Write your code here.
    vector<int> ds;
    solve(ds,num,num.size(),0);
    sort(ds.begin(),ds.end());
    return ds;
}
