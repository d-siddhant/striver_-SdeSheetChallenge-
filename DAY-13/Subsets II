#include<bits/stdc++.h> 
set<vector<int>> s;

void solve(int idx,int n, vector<int> &arr,vector<int> &ds, vector<vector<int>> &ans){
    
        ans.push_back(ds);
  
    for(int i=idx;i<n;i++){
        if(i!=idx and arr[i]==arr[i-1]) continue;
        ds.push_back(arr[i]);
        solve(i+1,n,arr,ds,ans);
        ds.pop_back();
             
    }
   
}
vector<vector<int>> uniqueSubsets(int n, vector<int> &arr)
{
    // Write your code here.
    vector<int> ds;
    vector<vector<int>> ans;
    sort(arr.begin(),arr.end());
    solve(0,n,arr,ds,ans);
   
    return ans;
}
