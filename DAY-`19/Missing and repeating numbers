
#include <bits/stdc++.h> 

pair<int,int> missingAndRepeating(vector<int> &arr, int n)
{
	// Write your code here 
	int ans1=-1,ans2=-1;
    sort(arr.begin(),arr.end());
   
    for(int i=1;i<=n-1;i++) {
        if(arr[i]==arr[i-1]) {
           ans2=arr[i];
            break;
        }
    }
    int sum=accumulate(arr.begin(),arr.end(),0);
    sum-=ans2;
    int temp=(n*(n+1))/2;
    ans1=temp-sum;
    pair<int,int> p={ans1,ans2};
        return p;
}
